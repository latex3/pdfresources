              \pdfdict_put:nnx {l_hyp/annot/A/Launch}{Win}
                {<</P \l_@@_para_tmpa_tl /F \l_@@_filename_tmpa_tl >>}
            }
          \mode_leave_vertical:
          \pdfannot_dict_put:nnx {link/Launch}{A}{<<\pdfdict_use:n {l_hyp/annot/A/Launch}>>}
