                 {
                   \pdf_bdcobject:nn {OC}{@@/OCG/Print}
                   \hbox_overlap_right:n { \box_use:N \l_@@_tmpa_box }
                   \pdf_emc:
                   \pdf_bdcobject:nn {OC}{@@/OCG/View}
                   \group_begin:
                   \color_select:n { hyp/color/#1 }
                   \box_use_drop:N \l_@@_tmpa_box
